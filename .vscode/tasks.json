{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "Build cmd_line",
      "command": "/usr/bin/g++",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${workspaceFolder}/src/voropp/cmd_line.cc",
        "-g",
        "${workspaceFolder}/src/voropp/voro++.cc",
        "-o",
        "${workspaceFolder}/examples/cmd_line.out"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": "build",
      "detail": "Task generated by Debugger."
    },
    {
      "type": "cppbuild",
      "label": "Build voro_test_cli",
      "command": "/usr/bin/g++",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${workspaceFolder}/src/voro_test_cli.cc",
        "-g",
        "${workspaceFolder}/src/voropp/voro++.cc",
        "-o",
        "${workspaceFolder}/src/voro_test_cli.out"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": "build",
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}